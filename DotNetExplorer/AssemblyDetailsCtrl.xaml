<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DotNetExplorer.AssemblyDetailsCtrl">
  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="5">
    <TextBlock x:Name="TbTitle" HorizontalAlignment="Left" FontSize="18">Assembly text</TextBlock>
    <TextBlock x:Name="TbInfo" HorizontalAlignment="Left">Assembly text</TextBlock>
  </StackPanel>
</UserControl>
