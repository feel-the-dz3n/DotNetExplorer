    <Rectangle Width="16" Height="16" Fill="#FFBBBBBB"></Rectangle>
    <TextBlock x:Name="TbNamespace" Margin="5,0,0,0" Opacity="0.75">Some.Namespace.</TextBlock>
    <TextBlock x:Name="TbType">Type</TextBlock>
  </WrapPanel>
</UserControl>
