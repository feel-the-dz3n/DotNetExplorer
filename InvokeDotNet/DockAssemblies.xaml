<UserControl x:Class="InvokeDotNet.DockAssemblies"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InvokeDotNet"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <local:DockWindow/>
        <Grid Margin="5">
            <TreeView x:Name="treeView" Margin="0,0,0,0">
            </TreeView>
            <Button x:Name="btnLoad" Click="BtnLoad_Click" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="10" Width="35" Height="35">+</Button>
            <!--<WrapPanel Margin="0,0,0,5" VerticalAlignment="Bottom" ItemWidth="25">
            </WrapPanel>-->
        </Grid>
    </Grid>
</UserControl>
